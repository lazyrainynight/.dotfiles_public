{
    "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
    "version": 2,
    "final_space": true,
    "blocks": [
        {
            "type": "prompt",
            "alignment": "left",
            "segments": [
                {
                    "type": "kubectl",
                    "style": "diamond",
                    "powerline_symbol": "\ue0b0",
                    "foreground": "#234",
                    "background": "#1075b4",
                    "leading_diamond": "\ue0b2"
                },
                {
                    "type": "dotnet",
                    "style": "powerline",
                    "powerline_symbol": "\ue0b0",
                    "foreground": "#234",
                    "background": "#83769c"
                },
                {
                    "type": "python",
                    "style": "powerline",
                    "powerline_symbol": "\ue0b0",
                    "foreground": "#234",
                    "background": "#83769c",
                    "properties": {
                        "display_mode": "context",
                        "fetch_version": true,
                        "fetch_virtual_env": true
                    }
                },
                {
                    "type": "node",
                    "style": "powerline",
                    "powerline_symbol": "\ue0b0",
                    "foreground": "#234",
                    "background": "#83769c"
                },
                {
                    "type": "go",
                    "style": "powerline",
                    "powerline_symbol": "\ue0b0",
                    "foreground": "#234",
                    "background": "#83769c",
                    "properties": {
                        "fetch_version": true
                    }
                },
                {
                    "type": "julia",
                    "style": "powerline",
                    "powerline_symbol": "\ue0b0",
                    "foreground": "#234",
                    "background": "#83769c",
                    "properties": {
                        "fetch_version": true
                    }
                },
                {
                    "type": "time",
                    "style": "powerline",
                    "powerline_symbol": "\ue0b0",
                    "foreground": "#234",
                    "background": "#e69270",
                    "properties": {
                        "time_format": "3:04:05 PM"
                    }
                },
                {
                    "type": "path",
                    "style": "powerline",
                    "powerline_symbol": "\ue0b0",
                    "foreground": "#234",
                    "background": "#33658A",
                    "properties": {
                        "style": "full"
                    }
                },
                {
                    "type": "git",
                    "style": "powerline",
                    "powerline_symbol": "\ue0b0",
                    "foreground": "#234",
                    "background": "#56865c",
                    "background_templates": [
                        "{{ if or (.Working.Changed) (.Staging.Changed) }}#eecc32{{ end }}",
                        "{{ if and (gt .Ahead 0) (gt .Behind 0) }}#dd5050{{ end }}",
                        "{{ if gt .Ahead 0 }}#b1967b{{ end }}",
                        "{{ if gt .Behind 0 }}#e4bf6f{{ end }}"
                    ],
                    "properties": {
                        "azure_devops_icon": "",
                        "bitbucket_icon": "",
                        "branch_ahead_icon": "\u2191",
                        "branch_behind_icon": "\u2193",
                        "branch_gone_icon": "\u2262",
                        "branch_icon": "\ue0a0 ",
                        "branch_identical_icon": "\u2261",
                        "cherry_pick_icon": "\u2713 ",
                        "commit_icon": "\u25b7 ",
                        "fetch_stash_count": true,
                        "fetch_status": true,
                        "fetch_upstream_icon": true,
                        "github_icon": "",
                        "gitlab_icon": "",
                        "merge_icon": "\u25f4 ",
                        "rebase_icon": "\u2c62 ",
                        "tag_icon": "\u25b6 "
                    }
                }
            ]
        }
    ]
}
